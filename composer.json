{
    "name": "fidum/padlock",
    "description": "A Laravel Zero Command for encrypting and decrypting environment files.",
    "keywords": ["framework", "laravel", "laravel zero", "console", "cli"],
    "homepage": "https://fidum.uk",
    "type": "project",
    "license": "MIT",
    "support": {
        "issues": "https://github.com/fidum/padlock/issues",
        "source": "https://github.com/fidum/padlock"
    },
    "authors": [
        {
            "name": "Daniel Mason",
            "email": "fidum.dev@gmail.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/encryption": "^12.0",
        "laravel-zero/phar-updater": "^1.4",
        "nunomaduro/termwind": "^2.3"
    },
    "require-dev": {
        "laravel-zero/foundation": "^12.0",
        "laravel-zero/framework": "^12.0",
        "laravel/pint": "^1.22",
        "mockery/mockery": "^1.6",
        "pestphp/pest": "^3.8"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "bin": ["builds/padlock"]
}
